{
  "name": "wl-draw",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@8.15.3",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check --write .",
    "check:ci": "biome ci .",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm"
  },
  "lint-staged": {
    "*.{js,ts,vue,json,css,scss}": [
      "biome check --write"
    ]
  },
  "engines": {
    "npm": "Use pnpm instead",
    "yarn": "Use pnpm instead",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@leafer-in/arrow": "^1.10.1",
    "@leafer-in/editor": "^1.10.1",
    "@leafer-in/export": "1.10.1",
    "@leafer-in/text-editor": "1.10.1",
    "@leafer-in/view": "1.10.1",
    "@leafer-in/viewport": "1.10.1",
    "@vueuse/core": "^14.0.0",
    "leafer-ui": "^1.10.1",
    "leafer-x-easy-snap": "^1.11.0",
    "naive-ui": "^2.43.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@iconify/json": "^2.2.410",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "only-allow": "^1.2.1",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "npm:rolldown-vite@7.2.7",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.5"
  }
}
